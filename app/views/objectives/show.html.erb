<div class="page-header">
  <%= link_to objectives_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_objective_path(@objective), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show objective</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @objective.name %></dd>

  <dt>Section:</dt>
  <dd><%= @objective.section %></dd>

  <dt>Percentage:</dt>
  <dd><%= @objective.percentage %></dd>

</dl>
